{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "Bundler",
    "rootDir": ".",
    "outDir": "dist",
    "declaration": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true
  },
  "include": ["engine/**/*.ts", "data/**/*.ts"],
  "exclude": ["dist", "tests"]
}
